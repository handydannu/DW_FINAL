<UserControl x:Class="BAPPEDADW.ETL.ETL"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <Label Content="EKSTRAK DATA" FontWeight="Bold" HorizontalAlignment="Center" Padding="20" FontSize="15"/>
                <Button Name="eks" Padding="10" Content="EKSTRAK" FontSize="20" Width="139" FontWeight="ExtraBlack" Click="Button_Click"/>
                <TextBlock Name="lblstatus" Text="" FontSize="15" FontWeight="Bold" HorizontalAlignment="Center" Padding="20"/>
            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
